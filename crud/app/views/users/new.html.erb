<h1>New robot</h1>
<!--%= error_messages_for :user %-->

<%= form_for @user ,:url => {:action => 'create'} do |f| %>
  <p>
    Name: <%= f.text_field 'name' %>
  </p>
   <p>
    Age: <%= f.text_field 'age' %>
  </p>
   <p>
    Place: <%= f.text_field 'place' %>
  </p>

  <p>
    Number: <%= f.text_field 'number' %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', :action => nil %>
