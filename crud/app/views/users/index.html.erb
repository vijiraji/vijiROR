<h1>Listing users</h1>

<table>
  <tr>Name</tr>
  <tr>Age</tr>
  <tr>Place</tr>
  <tr>Number</tr>

<% @user.each do |user| %>
  <tr>
     <td><%= user.name%></td>
     <td><%= user.age%></td>
     <td><%= user.place%></td>
     <td><%= user.number%></td>
    <td><%= link_to 'show', :action => 'show', :id => user.id %></td>
    <td><%= link_to 'Edit', :action => 'edit', :id => user.id %></td>
  <td> <%= link_to 'Delete', users_delete_path(user), method: :post %> </td>
 </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', :action => 'new' %>
